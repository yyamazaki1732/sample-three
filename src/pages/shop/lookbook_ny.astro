    font-size: 11px;
    width: 64px;
    border-right: 1px solid #e4e4e4;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
  }

  .header_langSet span a {
    display: block;
    font-size: 10px;
    width: 32px;
    height: 32px;
    line-height: 32px;
    border-left: 1px solid #e4e4e4;
    cursor: pointer;
  }

  .header_langSet span a.is-active {
    background: #3b3c3c;
    color: #fff;
  }

