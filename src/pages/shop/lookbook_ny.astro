  /* _pageloading */
  #pageloading {
    z-index: 9999;
    position: fixed;
    left: 0;
    top: 0;
    display: block;
    width: 100%;
    height: 100%;
    background: #fff;
  }

  #pageloading .pageloading_logo {
    display: none;
    animation: b 2s ease 0s 1 normal;
    -webkit-animation: b 2s ease 0s 1 normal;
    position: absolute;
    left: 50%;
    top: 50%;
    -webkit-transform: translate(-50%, -100%);
    transform: translate(-50%, -100%);
    width: 100%;
    height: 100%;
  }

  #pageloading .pageloading_logo span {
    overflow: hidden;
    display: block;
    width: 100%;
    height: 100%;
    padding-bottom: 55%;
    font-size: 0;
    line-height: 0;
    text-indent: -9999px;
    background-repeat: no-repeat;
    background-position: 50%;
    background-size: 30% !important;
    background-image: url("/img/usr/loading_text/img_message2211.png");
  }

  #pageloading .pageloading_copy {
    display: none;
    animation: c 2s ease 0s 1 normal;
    -webkit-animation: c 2s ease 0s 1 normal;
    position: absolute;
    left: 50%;
    bottom: 80px;
    -webkit-transform: translateX(-50%);
    transform: translateX(-50%);
    width: 12.85714%;
  }

  @-webkit-keyframes b {
    0% {
      top: 55%;
      opacity: 0;
    }

    to {
      top: 50%;
      opacity: 1;
    }
  }

  @-webkit-keyframes c {
    0% {
      opacity: 0;
    }

    to {
      opacity: 1;
    }
  }

  @-webkit-keyframes b {
    0% {
      top: 55%;
      opacity: 0;
    }

    to {
      top: 50%;
      opacity: 1;
    }
  }

  @keyframes b {
    0% {
      top: 55%;
      opacity: 0;
    }

    to {
      top: 50%;
      opacity: 1;
    }
  }

  @-webkit-keyframes c {
    0% {
      opacity: 0;
    }

    to {
      opacity: 1;
    }
  }

  @keyframes c {
    0% {
      opacity: 0;
    }

    to {
      opacity: 1;
    }
  }

  .wrapper_ {
    width: 100%;
    min-width: 1400px;
    margin: 0 auto;
    text-align: center;
    position: relative;
    padding-top: 110px;
    left: 0;
    -webkit-transition: left 300ms 0s ease;
    transition: left 300ms 0s ease;
  }

  .wrapper_.opened {
    position: fixed;
    left: 414px;
  }

  #header {
    width: 100%;
    height: 111px;
    zoom: 1;
    text-align: center;
    position: fixed;
    z-index: 2;
    background: #fff;
    left: 0;
    top: 0;
    -webkit-transition: left 300ms 0s ease;
    transition: left 300ms 0s ease;
  }

  #header #header_entry {
    float: left;
    font-size: 11px;
    width: auto;
  }

  #header #header_entry a {
    display: block;
    height: 32px;
    line-height: 32px;
  }

  #header #header_search_box {
    float: right;
    width: auto;
  }

  #header .header_inner_ {
    min-width: 1100px;
    margin: 0 auto;
    text-align: center;
    position: relative;
    width: 100%;
    height: 78px;
    padding: 0;
  }

  #header .header_inner_ .header_menu_btn_wrap_ {
    position: absolute;
    left: 40px;
    top: 31px;
  }

  #header .header_inner_ #header_menu_btn {
    position: relative;
    font-size: 11px;
    background: transparent;
    border: none;
    z-index: 1;
    cursor: pointer;
    padding-left: 38px;
    outline: none;
  }

  #header .header_inner_ #header_menu_btn:active,
  #header .header_inner_ #header_menu_btn:focus,
  #header .header_inner_ #header_menu_btn:active > p,
  #header .header_inner_ #header_menu_btn:focus > p {
    position: relative;
  }

  #header .header_inner_ #header_menu_btn span {
    position: absolute;
    width: 18px;
    height: 2px;
    left: 0;
    background: #3c3c3c;
  }

  #header .header_inner_ #header_menu_btn span:nth-child(1) {
    top: 0;
  }

  #header .header_inner_ #header_menu_btn span:nth-child(2) {
    top: 6px;
  }

  #header .header_inner_ #header_menu_btn span:nth-child(3) {
    top: 12px;
  }

  #header .header_inner_ #sitelogo {
    position: absolute;
    left: 0;
    right: 0;
    top: 19px;
    margin: 0 auto;
    float: none;
    width: auto;
    height: auto;
  }

  #header ul.customer_menu_ {
    position: absolute;
    right: 40px;
    top: 18px;
    margin: auto 0;
  }

  #header ul.customer_menu_ li {
    margin: 0 0 0 10px;
    float: left;
    margin-left: 25px;
    font-size: 11px;
  }

  #header ul.customer_menu_ li a {
    display: inline-block;
  }

  #header ul.customer_menu_ li#menu_wish a {
    padding-left: 24px;
    background: url("/img/usr/common/ico_heart.png") left center no-repeat;
  }

  #header ul.customer_menu_ li#menu_cart a {
    padding-left: 24px;
    background: url("/img/usr/common/ico_cart.png") left center no-repeat;
  }

  #header #headernav {
    width: 100%;
    height: 33px;
    min-width: 1100px;
    background: #fff;
    border-bottom: 1px solid #e4e4e4;
    /*position: fixed;z-index: 2;top: 0;left: 0;*/
    -webkit-transition: all 300ms 0s ease;
    transition: all 300ms 0s ease;
  }

  #header.opened {
    left: 414px;
  }

  #header #header_global_nav {
    display: inline-block;
    letter-spacing: -0.4em;
    -webkit-transform: translateX(81px);
    transform: translateX(81px);
  }

  #header #header_global_nav li {
    display: inline-block;
    letter-spacing: normal;
  }

  #header #header_global_nav li a {
    display: inline-block;
    font-size: 10px;
    width: 120px;
    height: 32px;
    line-height: 32px;
    border-left: 1px solid #e4e4e4;
  }

  #header #header_global_nav li:last-child a {
    border-right: 1px solid #e4e4e4;
  }

  /* ----ユーザー定義　グローバルナビゲーション---- */
  #headernav .header_dropdown_wrapper_ {
    display: none;
    position: absolute;
    z-index: 1;
    top: 33px;
    left: 0;
    right: 0;
    margin: 0 auto;
    width: 1000px;
    height: 278px;
    background: #fff;
    padding: 40px 102px 0;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    text-align: left;
    letter-spacing: normal;
  }

  #headernav #header_men_content {
    background: #fff url("/img/usr/nav/header_nav_men.jpg?v=20201210") 750px
      center no-repeat;
  }

  #headernav #header_women_content {
    background: #fff url("/img/usr/nav/header_nav_women.jpg?v=20201210") 750px
      center no-repeat;
  }

  #headernav .header_dropdown_title_ {
    font-size: 16px;
    font-weight: bold;
  }

  #headernav #header_dropdown_brand_list {
    margin-top: 28px;
    letter-spacing: -0.4em;
  }

  #headernav #header_dropdown_brand_list li {
    display: inline-block;
    letter-spacing: normal;
  }

  .header_dropdown_wrapper_#header_brand_content
    #header_dropdown_brand_list
    li {
    margin-bottom: 20px;
    vertical-align: top;
    width: 180px;
  }

  #headernav #header_dropdown_brand_list li + li {
    margin-left: 20px;
  }

  .header_dropdown_wrapper_#header_brand_content
    #header_dropdown_brand_list
    li:nth-child(4n + 1) {
    margin-left: 0;
  }

  #headernav .brand_name_ {
    font-size: 14px;
    margin-top: 10px;
  }

  #headernav .header_dropdown_list {
    margin-top: 45px;
    letter-spacing: -0.4em;
    width: 600px;
  }

  #headernav .header_dropdown_list li {
    display: inline-block;
    letter-spacing: normal;
    width: 150px;
    margin-bottom: 20px;
  }

  /* ----ユーザー定義　TOPページヘッダー 検索---- */
  #header_search {
    width: 100%;
    margin-bottom: 20px;
  }

  #header_search .header_inner_ {
    width: 960px;
    height: 30px;
    margin: 0 auto;
    zoom: 1;
  }

  #header_search .header_inner_:after {
    content: "";
    display: block;
    height: 0;
    font-size: 0;
    clear: both;
    visibility: hidden;
  }

  #header_search_box:after,
  #header_search_keyward:after,
  #header_search_keyword ul:after {
    content: "";
    display: block;
    height: 0;
    font-size: 0;
    clear: both;
    visibility: hidden;
  }

  #header_search_box {
    float: right;
    zoom: 1;
  }

  #header_search_box input {
    margin: 0;
    padding: 0;
    float: left;
    color: #999;
  }

  #header_search_box input#keyword {
    width: 130px;
    padding: 11px 10px;
    border: none;
    font-size: 10px;
    border-right: 1px solid #e4e4e4;
    outline: none;
    background: none;
    border-radius: 0;
    height: auto;
    line-height: 1;
  }

  /* ----ユーザー定義　ビジュアル---- */
  #header #header_global_nav li a {
    display: inline-block;
    font-size: 10px;
    width: 120px;
    height: 32px;
    line-height: 32px;
    border-left: 1px solid #e4e4e4;
  }

  #header .header_inner_ #sitelogo img {
    width: 180px;
  }

  /* ----ユーザー定義　TOPページ---- */
  /* slider*/
  .slick-slide {
    position: relative;
  }

  #header_search_box input[type="text"]#keyword {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    height: 32px;
    text-align: left;
  }

  #header .header_inner_ #sitelogo {
    top: 19px;
  }

  #header .header_inner_ #sitelogo img {
    width: 180px;
  }

  .wrapper_#toppage #header {
    z-index: 2;
  }

  .wrapper_#toppage .container_ {
    position: static;
  }

  .wrapper_#toppage .left_information_ .btn_more_ {
    margin-bottom: 160px;
  }

  #header {
    position: fixed;
    z-index: 100;
  }

  #header:not(.opened) {
    -webkit-transition: none;
    transition: none;
  }

  #header .header_inner_ {
    background: #fff;
  }

  .header_dropdown_wrapper_#header_brand_content {
    height: auto;
    padding-bottom: 20px;
  }

  .header_dropdown_wrapper_#header_brand_content
    #header_dropdown_brand_list
    li {
    margin-bottom: 20px;
    vertical-align: top;
    width: 180px;
  }

  .header_dropdown_wrapper_#header_brand_content
    #header_dropdown_brand_list
    li:nth-child(4n + 1) {
    margin-left: 0;
  }

  #header #header_global_nav {
    -webkit-transform: translateX(81px);
    transform: translateX(81px);
  }

